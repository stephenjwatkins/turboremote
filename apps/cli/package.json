{
  "name": "turboremote",
  "version": "0.0.0",
  "description": "Command-line tool for managing Turboremote connections",
  "homepage": "https://turboremote.org",
  "license": "UNLICENSED",
  "bin": {
    "turboremote": "dist/index.js"
  },
  "scripts": {
    "dev": "tsup --watch --env.API_URL \"http://localhost:3001\" --env.SERVER_URL \"http://localhost:3000\"",
    "build": "tsup --env.API_URL \"https://api.turboremote.org\" --env.SERVER_URL \"https://server.turboremote.org\"",
    "check-types": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "turboremote",
    "turborepo",
    "remote cache",
    "cli"
  ],
  "author": "Stephen Watkins (stephen@turboremote.org)",
  "dependencies": {
    "chalk": "4",
    "cli-spinners": "^2.7.0",
    "configstore": "^5.0.0",
    "directories-js": "^0.1.0",
    "inquirer": "^8.0.0",
    "node-fetch": "^2.6.2",
    "ora": "5",
    "prompts": "^2.4.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@turboremote/tsconfig": "*",
    "@types/configstore": "^5.0.0",
    "@types/inquirer": "^9.0.1",
    "@types/node": "^18.7.16",
    "@types/node-fetch": "^2.6.2",
    "@types/yargs": "^17.0.12",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "files": [
    "dist"
  ]
}
