{
  "name": "@turboremote/server",
  "version": "0.0.0",
  "private": true,
  "description": "Remote Cache Server for Turboremote",
  "main": "dist/index.js",
  "scripts": {
    "dev": "PORT=3000 ts-node-dev --respawn --transpile-only ./src/index.ts",
    "build": "tsup",
    "start": "node ./dist/index.js",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "turboremote",
    "turborepo",
    "remote cache",
    "server"
  ],
  "author": "Stephen Watkins <stjowa@gmail.com>",
  "devDependencies": {
    "@turboremote/tsconfig": "*",
    "@types/node": "^18.7.16",
    "pino-pretty": "^9.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@fastify/sensible": "^5.1.1",
    "@sentry/node": "^7.15.0",
    "@turboremote/lib": "*",
    "aws-sdk": "^2.1217.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.2",
    "fastify": "4.7",
    "fs-blob-store": "^6.0.0",
    "knex": "^2.3.0",
    "pg": "^8.8.0",
    "s3-blob-store": "^4.1.1",
    "uuid-base64": "^1.0.0"
  }
}
